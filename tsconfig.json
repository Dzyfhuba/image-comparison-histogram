{
  "compilerOptions": {
      "allowJs": true,
      "module": "ESNext",
      "moduleResolution": "node",
      "jsx": "react",
      "strict": true,
      "isolatedModules": true,
      "target": "ESNext",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "paths": {
          "@/*": ["./resources/js/*"]
      },
      "skipLibCheck": true,
  },
  "include": ["resources/js/**/*.ts", "resources/js/**/*.tsx", "resources/js/**/*.d.ts", "resources/js/ssr.jsx"],
  "exclude": ["node_modules"]
}

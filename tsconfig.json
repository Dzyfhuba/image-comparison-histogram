{
  "compilerOptions": {
        "target": "ESNext",
        "jsx": "react",
        "module": "ESNext",
        "moduleResolution": "node",
        "types": ["vite/client"],
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "paths": {
            "@/*": ["./resources/js/*"]
        }
  }
}
